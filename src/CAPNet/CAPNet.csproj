<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Matt Chandler, Teamnet, Damian Hintz</Authors>
    <Version>3.0.2</Version>
    <PackageId>DotNet.CommonAlertingProtocol</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A Common Alerting Protocol library for the .NET framework.</Description>
    <PackageLicenseUrl>http://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/damianhintz/cap-net</PackageProjectUrl>
    <PackageIconUrl>http://docs.oasis-open.org/emergency/edxl-cap-logo/v1.0/cnd01/CAP-Logos/CAP-Icon-L.png</PackageIconUrl>
    <PackageTags>CAP</PackageTags>
    <PackageReleaseNotes>Fixed: parsing of resource relative uri</PackageReleaseNotes>
    <Product>DotNet.CommonAlertingProtocol</Product>
    <RootNamespace>CAPNet</RootNamespace>
    <AssemblyName>DotNet.CommonAlertingProtocol</AssemblyName>
  </PropertyGroup>

</Project>
